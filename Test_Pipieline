pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Build') {
            steps {
                echo 'Build Stage- Building'
            }
        }
         stage('Deploy') {
            steps {
                echo 'Deploy Stage- Deploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Test Stage- Testing'
            }
        }
        stage('Release') {
            steps {
                echo 'Release Stage- Releasing'
            }
        }
    }
}
